                                    <span class="btn-text">@Localizer["go_to_login_page"]</span>
                                    <span class="btn-icon"><i class="feather-arrow-right"></i></span>
                                    <span class="btn-icon"><i class="feather-arrow-right"></i></span>
                                </span>
                            </a>
                            <a class="rbt-btn hover-icon-reverse btn-white" asp-action="ContactUs"
                                asp-controller="Main">
                                <span class="icon-reverse-wrapper">
                                    <span class="btn-text">@Localizer["go_to_contact_us_page"]</span>
                                    <span class="btn-icon"><i class="feather-arrow-right"></i></span>
                                    <span class="btn-icon"><i class="feather-arrow-right"></i></span>
                                </span>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
