@model int;
<a class="close_side_menu" href="javascript:void(0);"></a>

<!-- Start breadcrumb Area -->
<div class="rbt-breadcrumb-default ptb--100 ptb_md--50 ptb_sm--30 bg-gradient-1">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="breadcrumb-inner text-center">
                    <h2 class="title">@Localizer["our_projects"]</h2>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End Breadcrumb Area -->

<div class="rbt-about-area about-style-1 bg-color-white rbt-section-gap">
    <div class="container">
        <div class="row g-5 align-items-center">
            <div class="col-lg-5">
                <div class="content">
                    @if (Model == 0)
                    {
                        <iframe class="w-100 radius-10" width="1008" height="567"
                            src="https://www.youtube.com/embed/tQaqrLKDIjw"
                            title="Fanur Group" frameborder="0"
                            allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
                            referrerpolicy="strict-origin-when-cross-origin" allowfullscreen></iframe>
                    }else if(Model == 1){
                        <iframe class="w-100 radius-10" width="1008" height="567"
                        src="https://www.youtube.com/embed/DlheYwc0TbU?list=RDDlheYwc0TbU" 
                        title="Будь моим нарядом, сладким шоколадом. ПРЕМЬЕРА ТРЕКА" frameborder="0" 
                        allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" 
                        referrerpolicy="strict-origin-when-cross-origin" allowfullscreen></iframe>
                    }else if(Model == 2){
                        <iframe class="w-100 radius-10" width="1008" height="567" 
                        src="https://www.youtube.com/embed/uVCDKfV69wA?list=RDMMuVCDKfV69wA" 
                        title="G&#39;aybulla Tursunov - Janona (Premyera)" frameborder="0" 
                        allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" 
                        referrerpolicy="strict-origin-when-cross-origin" allowfullscreen></iframe>
                    }else if(Model == 3){
                        <iframe class="w-100 radius-10" width="1008" height="567" 
                        src="https://www.youtube.com/embed/6O34VWIIWe0?list=RD6O34VWIIWe0" 
                        title="VARSAL  ◣ Признание в любви ◥ChristoforosOrfanidis" frameborder="0" 
                        allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" 
                        referrerpolicy="strict-origin-when-cross-origin" allowfullscreen></iframe>
                    }else if(Model == 4){
                        <iframe class="w-100 radius-10" width="1008" height="567" 
                        src="https://www.youtube.com/embed/W64yrqCehuc?list=RD6O34VWIIWe0" 
                        title="Javid - Ты моя Дунья (Official Video)" frameborder="0" 
                        allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" 
                        referrerpolicy="strict-origin-when-cross-origin" 
                        allowfullscreen></iframe>
                    }else if(Model == 5){
                        <iframe class="w-100 radius-10" width="1008" height="567" 
                        src="https://www.youtube.com/embed/WVl6g5hvcDA?list=RD6O34VWIIWe0" 
                        title="Doston Ergashev - Ko&#39;zmunchog&#39;im | Достон Эргашев - Кузмунчогим" frameborder="0" 
                        allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" 
                        referrerpolicy="strict-origin-when-cross-origin" allowfullscreen></iframe>
                    }
                </div>
            </div>
            <div class="col-lg-7">
                <div class="inner pl--50 pl_sm--0 pl_md--0 pl_lg--0">
                    <div class="section-title text-start">
                        <h2 class="title">
                            @if (Model == 0)
                            {
                                @Localizer["complete_projects"]
                            }
                            else
                            {                                
                                @Localizer[$"{Model}_complete_projects"]
                            }
                        </h2>
                        <p class="description mt--20">
                            <strong>
                            @if (Model == 0)
                            {
                                @Localizer["complete_projects_description"]
                            }
                            else
                            {                                
                                @Localizer[$"{Model}_complete_projects_description"]
                            }
                            </strong>
                        </p>
                        <h5 class="mb--20">
                            @if (Model == 0)
                            {
                                @Localizer["complete_projects_title"]
                            }
                            else
                            {                                
                                @Localizer[$"{Model}_complete_projects_title"]
                            }
                        </h5>
                        <div class="plan-offer-list-wrapper">
                            <ul class="plan-offer-list">
                                <li><i class="feather-check"></i>
                                    @if (Model == 0)
                                    {
                                        @Localizer["complete_projects_first"]
                                    }
                                    else
                                    {                                
                                        @Localizer[$"{Model}_complete_projects_first"]
                                    }
                                </li>
                                <li><i class="feather-check"></i>
                                    @if (Model == 0)
                                    {
                                        @Localizer["complete_projects_second"]
                                    }
