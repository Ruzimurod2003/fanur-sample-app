                                    {                                
                                        @Localizer[$"{Model}_complete_projects_second"]
                                    }
                                </li>
                                <li><i class="feather-check"></i>
                                    @if (Model == 0)
                                    {
                                        @Localizer["complete_projects_third"]
                                    }
                                    else
                                    {                                
                                        @Localizer[$"{Model}_complete_projects_third"]
                                    }                                
                                </li>
                                <li><i class="feather-check"></i>
                                    @if (Model == 0)
                                    {
                                        @Localizer["complete_projects_fourth"]
                                    }
                                    else
                                    {                                
                                        @Localizer[$"{Model}_complete_projects_fourth"]
                                    }                                
                                </li>
                                <li><i class="feather-check"></i>
                                    @if (Model == 0)
                                    {
                                        @Localizer["complete_projects_fife"]
                                    }
                                    else
                                    {                                
                                        @Localizer[$"{Model}_complete_projects_fife"]
                                    }                                
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="rbt-separator-mid">
    <div class="container">
        <hr class="rbt-separator m-0">
    </div>
</div>