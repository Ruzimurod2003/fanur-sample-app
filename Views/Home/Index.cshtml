<!-- Start Slider Area  -->
<div class="rbt-splash-slider d-flex align-items-center">
    <div class="wrapper">
        <div class="container-fluid">
            <div class="row align-items-center">
                <div class="col-lg-12 col-xl-6 order-2 order-xl-1">
                    <div class="inner">
                        <h1 class="title" style="font-size: 50px;">
                            @Localizer["the_word_next_to_the_video_is_main_index"]
                        </h1>
                        <p class="description">
                            @Localizer["the_word_next_to_the_video_is_main_index_detail"]
                        </p>
                    </div>
                </div>
                <div class="col-lg-12 col-xl-6 order-1 order-xl-2">
                    <div class="video-popup-wrapper">
                        <iframe class="w-100 rbt-radius" width="1280" height="536"
                                src="https://www.youtube.com/embed/tQaqrLKDIjw"
                                title="test title" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" allowfullscreen></iframe>
                        <a class="rbt-btn rounded-player-2 popup-video position-to-top with-animation d-none" href="https://www.youtube.com/watch?v=nA1Aqp0sPQo">
                            <span class="play-icon"></span>
                        </a>
                        <div class="banner-group-shape">
                            <div class="shape-image scene shape-4">
                                <span data-depth="2">
                                    <img src="~/lib/main_template/assets/images/splash/icons/shape-4.png" alt="Shape Images">
                                </span>
                            </div>
                            <div class="shape-image scene shape-5">
                                <span data-depth="-2">
                                    <img src="~/lib/main_template/assets/images/splash/icons/shape-5.png" alt="Shape Images">
